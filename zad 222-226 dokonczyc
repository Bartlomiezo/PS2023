#include <stdio.h>
#include <stdlib.h>
#include <math.h>

void zad222(unsigned int a){
    int silnia=1;
    for(int i=1;i<=a;i++){
        silnia=silnia*i;
    }
    printf("Silnia z liczby %u wynosi %d",a,silnia);
}
void zad226(int n,m){
    if(n==0 and m==0){
        printf("Dwie rowne zero\n");
    }
    else{
        
    }
}
int main()
{
    unsigned int a;
    printf("Podaj liczbe nieujemna: ");
    scanf("%u",&a);
    zad222(a);
    int n,m;
    printf("Podaj dwie liczby: ");
    scanf("%d %d",&n,&m);
}
