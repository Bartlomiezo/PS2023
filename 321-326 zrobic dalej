#include <stdio.h>
#include <stdlib.h>

void zad321(int a, int b){
 if(&a>&b){
        printf("Mniejsza liczba to %d\n", b);
    }
    else{
        printf("Mniejsza liczba to %d\n", a);
    }
}
void zad322(int a, int b){
 if(&a>&b){
        printf("Adres mniejszej liczby to %p\n", &b);
    }
    else{
        printf("Adres mniejszej liczby to %p\n", &a);
    }
}
void zad324(int *a, int *b){
    int c;
    if(*b<*a){
        c=*b;
        *b=*a;
        *a=c;
    }
}
void zad326(int *n, int *w){
    *w=*n;
    printf("%p\n",&w);
}
void *zad329(int *p){
    
}
int main()
{
    int a,b;
    printf("Podaj dwie liczby: ");
   scanf("%d%d", &a, &b);
   zad321(a,b);
   zad322(a,b);
   zad324(&a,&b);
   printf("%d %d\n", a,b);
   int n;
   int* w;
   printf("podaj dwie liczby: ");
   scanf("%d %d",&n,&w);
   printf("%p %p\n",&n,&w);
   zad326(&n,&w);

}
